<?xml version='1.0' encoding='utf-8'?>
<widget id="org.slowrollbuffalo.app" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>Slow Roll Buffalo</name>
    <description>
        Slow Roll Buffalo App
    </description>
    <author email="tim@timduffy.me" href="http://timduffy.me/">
        Timothy Duffy
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
